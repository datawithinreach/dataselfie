{
	"env": {
	  "browser": true,
	  "es6": true,
	  "node": true
	},
	"extends": [
	  "eslint:recommended", "plugin:react/recommended"
	], 
	"settings": {
    "react":{
      "version":"16.4"
    }
  },
	"parserOptions": {
	  "ecmaVersion": 6,
	  "sourceType": "module",
	  "ecmaFeatures": {
		"experimentalObjectRestSpread": true
	  }
  
	},
	"rules": {
	  "process.env": ["off"],
	  "indent": [
		"error", "tab", {"SwitchCase": 1}
	  ],
	  "quotes": [
		"error", "single"
	  ],
	  "semi": [
		"error", "always"
	  ],
	  
	  "no-console": 0
	},
	"plugins": ["react"]
  }